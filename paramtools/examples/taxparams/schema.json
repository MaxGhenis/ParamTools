{
    "schema_name": "policy",
    "labels": {
        "year": {
            "type": "int",
            "validators": {"range": {"min": 2013, "max": 2027}}
        },
        "marital_status": {
            "type": "str",
            "validators": {"choice": {"choices": ["single", "joint", "separate",
                                                 "headhousehold", "widow"]}}
        },
        "idedtype": {
            "type": "str",
            "validators": {"choice": {"choices": ["medical", "statelocal",
                                                 "realestate", "casualty",
                                                 "misc", "interest", "charity"]}}
        },
        "EIC": {
            "type": "str",
            "validators": {"choice": {"choices": ["0kids", "1kid",
                                                 "2kids", "3+kids"]}}
        }
    },
    "optional": {
        "section_1": {"type": "str"},
        "section_2": {"type": "str"},
        "section_3": {"type": "str"},
        "irs_ref": {"type": "str"},
        "start_year": {"type": "int"},
        "cpi_inflatable": {"type": "bool"},
        "cpi_inflated": {"type": "bool"},
        "compatible_data": {"type": "compatible_data"}
    }
}